{
  "name": "obsidian-kindle-highlights-import-plugin",
  "version": "1.3.1",
  "description": "Plugin for Obsidian which converts Kindle highlights exports to notes",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && npm run biome:fix && git add manifest.json versions.json",
    "test": "jest --ci",
    "test:dev": "jest --watch",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "builtin-modules": "5.0.0",
    "esbuild": "0.27.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-util": "^30.0.5",
    "obsidian": "latest",
    "ts-jest": "^29.4.6",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  }
}
